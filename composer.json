{
  "name": "lloc/gutenberg-mvp",
  "description": "Gutenberg MVP",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "homepage": "http://lloc.de",
  "require": {
    "php": ">=7.2",
    "composer/installers": "~1.11.0"
  },
  "autoload": {
    "psr-4": {
      "Gmvp\\": "includes/Gmvp"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GmvpTests\\": "tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "coverage": "./vendor/bin/phpunit --coverage-html ./reports/php/coverage"
  },
  "authors": [
    {
      "name": "Dennis Ploetner",
      "email": "re@lloc.de",
      "homepage": "http://lloc.de"
    }
  ],
  "keywords": [
    "gutenberg",
    "mvp",
    "block",
    "schortcode",
    "widgets"
  ],
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "brain/monkey": "2.*",
    "phpstan/phpstan": "^1.9",
    "szepeviktor/phpstan-wordpress": "^1.1",
    "phpstan/extension-installer": "^1.2"
  }
}
